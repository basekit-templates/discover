{% extends "layout.twig" %}
{% block class %}theme-default{% endblock %}
{% block content %}

    <div class="container"> <!-- Container -->

        <div class="template-content">

            <!-- Header Include -->
            {% include "includes/header.twig" %}

        </div>

    </div><!-- End Container -->

    <!-- Navigation -->
    {{widget('extendednavigation', 'navigation', {"align":"center"})|raw}}

    <div class="container"> <!-- Container -->

        <div class="template-content default-content">

            <!-- Main Widget Zone -->
            {{zone('main')|raw}}

            <!-- Footer Include -->
            {% include "includes/footer.twig" %}

        </div>

    </div><!-- End Container -->

{% endblock %}
